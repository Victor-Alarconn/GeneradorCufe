<?xml version="1.0" encoding="UTF-8"?>
<Invoice xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2" xmlns:sts="dian:gov:co:facturaelectronica:Structures-2-1" xmlns:xades="http://uri.etsi.org/01903/v1.3.2#" xmlns:xades141="http://uri.etsi.org/01903/v1.4.1#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2     http://docs.oasis-open.org/ubl/os-UBL-2.1/xsd/maindoc/UBL-Invoice-2.1.xsd">
	<ext:UBLExtensions>
		<ext:UBLExtension>
            <ext:ExtensionContent>
                <sts:DianExtensions>
					<sts:InvoiceControl>
						<sts:InvoiceAuthorization>{InvoiceAuthorization}</sts:InvoiceAuthorization>
						<sts:AuthorizationPeriod>
							<cbc:StartDate>{StartDate}</cbc:StartDate>
							<cbc:EndDate>{EndDate}</cbc:EndDate>
						</sts:AuthorizationPeriod>
						<sts:AuthorizedInvoices>
							<sts:Prefix>{Prefix}</sts:Prefix>
							<sts:From>{From}</sts:From>
							<sts:To>{To}</sts:To>
						</sts:AuthorizedInvoices>
                   </sts:InvoiceControl>
				</sts:DianExtensions>
          </ext:ExtensionContent>
        </ext:UBLExtension>
	</ext:UBLExtensions>	
	<cbc:CustomizationID>{CustomizationID}</cbc:CustomizationID> <!--Indicador del tipo de operación [1..1] tabla 6.1.5-->
	<cbc:ProfileExecutionID>{ProfileExecutionID}</cbc:ProfileExecutionID> <!-- Tipo de ambiente [1..1] Tabla 6.5.1.2 -->
	<cbc:ID>{ID}</cbc:ID> <!--Número de factura o factura cambiaria. Incluye prefijo + consecutivo de factura autorizados por la DIAN  [1..1] -->
	<cbc:UUID schemeID="2" schemeName="CUFE-SHA384">{UUID}</cbc:UUID> <!-- Definido en el numeral 10.1 Rechazo: Si el valor UUID no está correctamente calculad [1..1] Tabla 6.5.1.2 -->
	<cbc:IssueDate>{IssueDate}</cbc:IssueDate> <!-- Fecha de generación de la factura [1..1] formato aaaa-mm-dd-->
	<cbc:IssueTime>{IssueTime}</cbc:IssueTime> <!--Hora de emisión [1..1] -->
	<cbc:InvoiceTypeCode>{InvoiceTypeCode}</cbc:InvoiceTypeCode> <!--Tipo de Factura [1..1] Tabla 6.1.3 -->
	<cbc:Note>{Note}</cbc:Note> <!--Información adicional: Texto libre, relativo al documento [0..N] -->
	<cbc:DocumentCurrencyCode>{DocumentCurrencyCode}</cbc:DocumentCurrencyCode> <!--Divisa de la Factura: Divisa aplicable a toda la factura [1..1] Tabla 6.3.3  -->
	<cbc:LineCountNumeric>{LineCountNumeric}</cbc:LineCountNumeric> <!--Número o cantidad de elementos InvoiceLine de la factura [1..1]-->
	<cac:AccountingSupplierParty> <!--Grupo con información que definen el obligado a facturar: Emisor de la factura [1..1]-->
		<cbc:AdditionalAccountID>{AdditionalAccountID}</cbc:AdditionalAccountID>
		<cac:Party>
			<cac:PartyName>
				<cbc:Name>{PartyNameName}</cbc:Name>
			</cac:PartyName>
			<cac:PhysicalLocation>
				<cac:Address>
					<cbc:ID>{PhysicalLocationID}</cbc:ID>
					<cbc:CityName>{PhysicalLocationCity}</cbc:CityName>
					<cbc:PostalZone>{PhysicalLocationPostalZone}</cbc:PostalZone>
					<cbc:CountrySubentity>{PhysicalLocationCountrySubentity}</cbc:CountrySubentity>
					<cbc:CountrySubentityCode>{PhysicalLocationCountrySubentityCode}</cbc:CountrySubentityCode>
					<cac:AddressLine>
						<cbc:Line>{PhysicalLocationLine}</cbc:Line>
					</cac:AddressLine>
					<cac:Country>
						<cbc:IdentificationCode>CO</cbc:IdentificationCode>
						<cbc:Name languageID="es">Colombia</cbc:Name>
					</cac:Country>
				</cac:Address>
			</cac:PhysicalLocation>
			<cac:PartyTaxScheme>
				<cbc:RegistrationName>{RegistrationName}</cbc:RegistrationName>
				<cbc:CompanyID schemeID="0" schemeName="31" schemeAgencyID="195" schemeAgencyName="CO, DIAN (Dirección de Impuestos y Aduanas Nacionales)">{PartyTaxSchemeCompanyID}</cbc:CompanyID>
				<cbc:TaxLevelCode>{TaxLevelCode}</cbc:TaxLevelCode><!--Régimen Fiscal: cbc:TaxLevelCode.@listName 6.2.4.,+ Responsabilidades fiscales: cbc:TaxLevelCode 6.2.7.[0..1] -->
				<cac:RegistrationAddress>
					<cbc:ID>{PartyTaxSchemeID}</cbc:ID>
					<cbc:CityName>{PartyTaxSchemeCityName}</cbc:CityName>
					<cbc:PostalZone>{PartyTaxSchemePostalZone}</cbc:PostalZone>
					<cbc:CountrySubentity>{PartyTaxSchemeCountrySubentity}</cbc:CountrySubentity>
					<cbc:CountrySubentityCode>{PartyTaxSchemeCountrySubentity}</cbc:CountrySubentityCode>
					<cac:AddressLine>
						<cbc:Line>{PartyTaxSchemeLine}</cbc:Line>
					</cac:AddressLine>
					<cac:Country>
						<cbc:IdentificationCode>{CountryIdentificationCode}</cbc:IdentificationCode>
						<cbc:Name languageID="es">{CountryName}</cbc:Name>
					</cac:Country>
				</cac:RegistrationAddress>
				<cac:TaxScheme>
					<cbc:ID>{TaxSchemeID}</cbc:ID>
					<cbc:Name>{TaxSchemeName}</cbc:Name>
				</cac:TaxScheme>
			</cac:PartyTaxScheme>
			<cac:PartyLegalEntity>
				<cbc:RegistrationName>{RegistrationName}</cbc:RegistrationName>
				<cbc:CompanyID schemeID="{PartyLegalEntityDV}" schemeName="31" schemeAgencyID="195" schemeAgencyName="CO, DIAN (Dirección de Impuestos y Aduanas Nacionales)">{PartyLegalEntityCompanyID}</cbc:CompanyID>
				<cac:CorporateRegistrationScheme>
					<cbc:ID>{CorporateRegistrationSchemeID}</cbc:ID>
					<cbc:Name>{CorporateRegistrationSchemeName}</cbc:Name>
				</cac:CorporateRegistrationScheme>
			</cac:PartyLegalEntity>
			<cac:Contact>
				<cbc:ElectronicMail>{ContactElectronicMail}</cbc:ElectronicMail>
			</cac:Contact>
		</cac:Party>
	</cac:AccountingSupplierParty>
	<cac:AccountingCustomerParty>  <!--Grupo con información que definen el Adquiriente [1..1]-->
		<cbc:AdditionalAccountID>{AdditionalAccountID}</cbc:AdditionalAccountID> <!-- Grupo con información que definen el Adquiriente [1..1]-->
		   <cac:PartyIdentification> <!-- Grupo para informar el documento del adquirente [0..1]-->
            	<cbc:ID schemeName ="{PartyIdentificationschemeName}" >{PartyIdentificationID}</cbc:ID> <!--Identificación del adquirente, @schemeName Ver lista de valores posibles en la columna “Código” del numeral 6.2.1. [1..1] -->
    		</cac:PartyIdentification> 
		<cac:Party>
			<cac:PartyName>
				<cbc:Name>{AccountingCustomerPartyName}</cbc:Name>
			</cac:PartyName>
			<cac:PhysicalLocation>
				<cac:Address>
					<cbc:ID>{AccountingCustomerPartyID}</cbc:ID>
					<cbc:CityName>{AccountingCustomerPartyCityName}</cbc:CityName>
					<cbc:PostalZone>{AccountingCustomerPartyPostalZone}</cbc:PostalZone>
					<cbc:CountrySubentity>{AccountingCustomerPartyCountrySubentity}</cbc:CountrySubentity>
					<cbc:CountrySubentityCode>{AccountingCustomerPartyCountrySubentityCode}</cbc:CountrySubentityCode>
					<cac:AddressLine>
						<cbc:Line>{AccountingCustomerPartyLine}</cbc:Line>
					</cac:AddressLine>
					<cac:Country>
						<cbc:IdentificationCode>CO</cbc:IdentificationCode>
						<cbc:Name languageID="es">Colombia</cbc:Name>
					</cac:Country>
				</cac:Address>
			</cac:PhysicalLocation>
			<cac:PartyTaxScheme>
				<cbc:RegistrationName>{PartyTaxScheme}</cbc:RegistrationName>
				<cbc:CompanyID schemeID="PartyTaxSchemeDV" schemeName="31" schemeAgencyID="195" schemeAgencyName="CO, DIAN (Dirección de Impuestos y Aduanas Nacionales)">{PartyTaxSchemeCompanyID}</cbc:CompanyID>
				<cbc:TaxLevelCode>{PartyTaxSchemeTaxLevelCode}</cbc:TaxLevelCode> <!--Régimen Fiscal: cbc:TaxLevelCode.@listName 6.2.4.,+ Responsabilidades fiscales: cbc:TaxLevelCode 6.2.7.[0..1] -->
				<cac:RegistrationAddress>
					<cbc:ID>{PartyTaxSchemeID}</cbc:ID>
					<cbc:CityName>{PartyTaxSchemeCityName}</cbc:CityName>
					<cbc:PostalZone>{PartyTaxSchemePostalZone}</cbc:PostalZone>
					<cbc:CountrySubentity>{PartyTaxSchemeCountrySubentity}</cbc:CountrySubentity>
					<cbc:CountrySubentityCode>{PartyTaxSchemeCountrySubentityCode}</cbc:CountrySubentityCode>
					<cac:AddressLine>
						<cbc:Line>{PartyTaxSchemeLine}</cbc:Line>
					</cac:AddressLine>
					<cac:Country>
						<cbc:IdentificationCode>CO</cbc:IdentificationCode>
						<cbc:Name languageID="es">Colombia</cbc:Name>
					</cac:Country>
				</cac:RegistrationAddress>
				<cac:TaxScheme>
					<cbc:ID>{TaxSchemeID}</cbc:ID>
					<cbc:Name>{TaxSchemeName}</cbc:Name>
				</cac:TaxScheme>
			</cac:PartyTaxScheme>
			<cac:PartyLegalEntity>
				<cbc:RegistrationName>{PartyLegalEntityRegistrationName}</cbc:RegistrationName>
				<cbc:CompanyID schemeID="{PartyLegalEntityDV}" schemeName="31" schemeAgencyID="195" schemeAgencyName="CO, DIAN (Dirección de Impuestos y Aduanas Nacionales)">{PartyLegalEntityCompanyID}</cbc:CompanyID>
				<cac:CorporateRegistrationScheme>
					<cbc:Name>{PartyLegalEntityName}</cbc:Name>
				</cac:CorporateRegistrationScheme>
			</cac:PartyLegalEntity>
			<cac:Contact>
				<cbc:ElectronicMail>{PartyLegalEntityElectronicMail}</cbc:ElectronicMail>
				
			</cac:Contact>
		</cac:Party>
	</cac:AccountingCustomerParty>
	<cac:PaymentMeans> <!--Grupo de campos para información relacionadas con el pago de la factura. [1..N] -->
		<cbc:ID>{PaymentMeansID}</cbc:ID>
		<cbc:PaymentMeansCode>{PaymentMeansPaymentMeansCode}</cbc:PaymentMeansCode>
		<cbc:PaymentID>{PaymentMeansPaymentID}</cbc:PaymentID>
	</cac:PaymentMeans>
	<cac:TaxTotal> <!--Grupo de campos para información totales relacionadas con un tributo [0..N]-->
		<cbc:TaxAmount currencyID="COP">{TaxTotalaxAmount}</cbc:TaxAmount>
		<cac:TaxSubtotal>
			<cbc:TaxableAmount currencyID="COP">{TaxTotalTaxableAmount}</cbc:TaxableAmount>
			<cbc:TaxAmount currencyID="COP">{TaxTotalTaxAmount}</cbc:TaxAmount>
			<cac:TaxCategory>
				<cbc:Percent>{TaxTotalPercent}</cbc:Percent>
				<cac:TaxScheme>
					<cbc:ID>01</cbc:ID>
					<cbc:Name>IVA</cbc:Name>
				</cac:TaxScheme>
			</cac:TaxCategory>
		</cac:TaxSubtotal>
	</cac:TaxTotal>
	<cac:LegalMonetaryTotal> <!--Grupo de campos para información relacionadas con los valores totales aplicables a la factura [1..1]-->
		<cbc:LineExtensionAmount currencyID="COP">{LegalMonetaryTotalLineExtensionAmount}</cbc:LineExtensionAmount>
		<cbc:TaxExclusiveAmount currencyID="COP">{LegalMonetaryTotalTaxExclusiveAmount}</cbc:TaxExclusiveAmount>
		<cbc:TaxInclusiveAmount currencyID="COP">{LegalMonetaryTotalTaxInclusiveAmount}</cbc:TaxInclusiveAmount>
		<cbc:PayableAmount currencyID="COP">{LegalMonetaryTotalPayableAmount}</cbc:PayableAmount>
	</cac:LegalMonetaryTotal>
	<cac:InvoiceLine> <!--Grupo de campos para información relacionadas con una línea de factura [1..N] -->
		<cbc:ID>{InvoiceLineID}</cbc:ID>
		<cbc:InvoicedQuantity>{InvoiceLineInvoicedQuantity}</cbc:InvoicedQuantity>
		<cbc:LineExtensionAmount currencyID="COP">{InvoiceLineLineExtensionAmount}</cbc:LineExtensionAmount>
		<cac:TaxTotal>
			<cbc:TaxAmount currencyID="COP">{InvoiceLineTaxAmount}</cbc:TaxAmount>			
			<cac:TaxSubtotal>
				<cbc:TaxableAmount currencyID="COP">{InvoiceLineTaxableAmount}</cbc:TaxableAmount>
				<cbc:TaxAmount currencyID="COP">{InvoiceLineTaxAmount}</cbc:TaxAmount>
				<cac:TaxCategory>
					<cbc:Percent>{InvoiceLinePercent}</cbc:Percent>
					<cac:TaxScheme>
						<cbc:ID>{TaxSchemeID}</cbc:ID>
						<cbc:Name>TaxSchemeName</cbc:Name>
					</cac:TaxScheme>
				</cac:TaxCategory>
			</cac:TaxSubtotal>
		</cac:TaxTotal>
		<cac:Item>
			<cbc:Description>{ItemDescription}</cbc:Description>
			<cac:StandardItemIdentification>
				<cbc:ID schemeID="999">{ItemID}</cbc:ID>
			</cac:StandardItemIdentification>
		</cac:Item>
		<cac:Price>
			<cbc:PriceAmount currencyID="{PriceCurrencyID}">{PricePriceAmount}</cbc:PriceAmount>
			<cbc:BaseQuantity unitCode="{PriceBaseUnitCode}">{PriceBaseQuantity}</cbc:BaseQuantity>
		</cac:Price>
	</cac:InvoiceLine>
	<DATA>
		<UBL21>true</UBL21>
		<Partnership>
			<ID>{PartnershipID}</ID> <!--Identificación de la Alianza "A" [1..1] -->
			<TechKey>fc8eac422eba16e22ffd8c6f94b3f40a6e38162c</TechKey> <!--Clave Técnica [1..1]-->
			<SetTestID>{SetTestID}</SetTestID> <!-- TestID Sólo Aplica para pruebas [1..1] -->
		</Partnership>
	</DATA>
</Invoice>